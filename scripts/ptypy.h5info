#! /usr/bin/python

import sys

usage = """\
Usage:
%s any_ptypy_file.pty(r/d) 

Shows on screen a summary of content in that h5 compatible file.
""" % sys.argv[0]

if len(sys.argv) not in [2]:
    print usage
    sys.exit(0)

from ptypy import utils as u
from ptypy import io

filename = sys.argv[1]
print u.verbose.report(io.h5read(filename),noheader=True)
